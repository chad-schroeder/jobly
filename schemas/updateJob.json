{
  "definitions": {},
  "$schema": "",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "default": null,
  "minProperties": 1,
  "properties": {
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "The Title Schema",
      "default": "",
      "examples": ["Senior Engineer"],
      "minLength": 1,
      "pattern": "^(.*)$"
    },
    "salary": {
      "$id": "#/properties/salary",
      "type": "integer",
      "title": "The Salary Schema",
      "default": 0,
      "examples": [25000]
    },
    "equity": {
      "$id": "#/properties/equity",
      "type": "number",
      "title": "The Equity Schema",
      "default": 0.0,
      "examples": [0.2],
      "maximum": 1.0
    },
    "company_handle": {
      "$id": "#/properties/company_handle",
      "type": "string",
      "title": "The Company_handle Schema",
      "default": "",
      "examples": ["google"],
      "minLength": 1,
      "pattern": "^(.*)$"
    }
  }
}
