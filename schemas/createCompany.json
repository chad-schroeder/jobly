{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": ["handle", "name"],
  "properties": {
    "handle": {
      "$id": "#/properties/handle",
      "type": "string",
      "title": "The Handle Schema",
      "default": "",
      "examples": ["amazon"],
      "minLength": 1,
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": ["Amazon"],
      "minLength": 1,
      "pattern": "^(.*)$"
    },
    "num_employees": {
      "$id": "#/properties/num_employees",
      "type": "integer",
      "title": "The Num_employees Schema",
      "default": 0,
      "examples": [4000]
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": ["This is Amazon"],
      "pattern": "^(.*)$"
    },
    "logo_url": {
      "$id": "#/properties/logo_url",
      "type": "string",
      "title": "The Logo_url Schema",
      "default": "",
      "examples": [""],
      "pattern": "^(.*)$"
    },
    "token": {
      "$id": "#/properties/token",
      "type": "string",
      "title": "The Token Schema",
      "default": "",
      "examples": ["jhsdfjhsdjfhsdfjghsdfghgsdhgjkldfjghdjsgfhjgdjfhgjksdhfjk"],
      "pattern": "^(.*)$"
    }
  },
  "additionalProperties": false
}
